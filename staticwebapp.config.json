{
    "routes": [
        {
            "route": "/index.html",
            "rewrite": "/404.html"
        }
    ],
    "navigationFallback": 
        {
            "rewrite": "/404",
            "exclude": ["/resources/*.{png,jpg,gif}", "/css/*"]
        },
    "responseOverrides": 
        {
            "404": {
                "rewrite": "/404.html"
              }
        },
    "mimeTypes": 
        {
            ".json": "text/json",
            ".js": "text/javascript",
            ".css": "text/css"
        },
    "platform": {
    "apiRuntime": "node:16"
  }
}
