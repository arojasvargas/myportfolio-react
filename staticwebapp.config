{
    "routes": [
        {
            "route": "/index.html",
            "rewrite": "/siteunavailable.html"
        }
    ],
    "navigationFallback": 
        {
            "rewrite": "/siteunavailable",
            "exclude": ["/resources/*.{png,jpg,gif}", "/css/*"]
        },
    "responseOverrides": 
        {
            "404": {
                "rewrite": "/404.html"
              }
        },
    "mimeTypes": 
        {
            ".json": "text/json",
            ".js": "text/javascript",
            ".css": "text/css"
        }
}
